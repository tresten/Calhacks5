{
 "manifest_version": 2,
 "name": "CalHacks",
 "description": "Recommends something new to learn based on your interests",
 "version": "1",
 "author": "Does anyone care?",
"browser_action": {
   "default_title": "Get smarter",
   "default_popup" : "popup.html"
 },
"background": {
      "scripts": ["background.js"],
      "persistent": true
    },

"chrome_url_overrides" : {
  "newtab": "newtab.html"
},
 "permissions": ["tabs","webRequest", "webNavigation", "activeTab", "storage"]
}
